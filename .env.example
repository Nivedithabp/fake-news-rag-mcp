# Free AI Models Configuration
# No API keys required for basic usage!

# Embedding Configuration (Free)
EMBEDDING_PROVIDER=huggingface
HF_EMBED_MODEL=sentence-transformers/all-MiniLM-L6-v2
# Optional: HF_API_KEY=your_huggingface_token (for higher rate limits)

# LLM Configuration (Free)
LLM_PROVIDER=huggingface
HF_LLM_MODEL=microsoft/DialoGPT-medium
# Alternative: LLM_PROVIDER=rule-based (completely free, no API calls)

# Ollama Configuration (Local, Free)
# LLM_PROVIDER=ollama
# OLLAMA_BASE_URL=http://localhost:11434
# OLLAMA_MODEL=llama2

# Vector Database Configuration
VECTOR_DB_PROVIDER=chroma
# For Pinecone (when VECTOR_DB_PROVIDER=pinecone)
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENV=your_pinecone_environment_here

# Chroma Configuration (when VECTOR_DB_PROVIDER=chroma)
CHROMA_HOST=chroma
CHROMA_PORT=8000

# MCP Server Configuration
MCP_SERVER_URL=http://mcp-server:4000/mcp
MCP_SERVER_ADMIN_TOKEN=your_admin_token_here

# Frontend Configuration
NEXT_PUBLIC_API_MCP_PROXY=/api/mcp-proxy
PORT=3000

# Kaggle Configuration
KAGGLE_USERNAME=your_kaggle_username
KAGGLE_KEY=your_kaggle_api_key
